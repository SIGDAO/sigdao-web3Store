{
  "version": 2,
  "routes": [
    {
      "src": "/img/(.*)",
      "headers": {
        "cache-control": "s-maxage=604800",
        "connection": "keep-alive"
      },
      "dest": "/img/$1"
    }
  ],
  "functions": {
    "api/*.js": {
      "memory": 512,
      "runtime": "vercel-sapper"
    }
  }
}
